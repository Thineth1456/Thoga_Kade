	<parameter name="orderIds" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT 
 od.itemCode,
 od.qty,
 od.unitPrice,
 od.orderId,
 itm.description,
 (od.qty * od.unitPrice) AS total
FROM orderdetail od
INNER JOIN item itm ON od.itemCode = itm.code
WHERE od.orderId =$P{orderIds}]]></query>
